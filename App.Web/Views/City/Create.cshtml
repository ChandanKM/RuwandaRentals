@model App.Web.ViewModels.CityViewModel

@{
    ViewBag.Title = "LMK Maintenance";
}

@*Scripts for create*@
<link href="~/Content/font-awesome.css" rel="stylesheet" />
<link href="@Url.Content("~/Content/themes/base/jquery.ui.all.css")" rel="stylesheet" type="text/css" />
<link href="~/Content/error-notifier.css" rel="stylesheet" />

@Scripts.Render("~/bundles/modernizr")
@Scripts.Render("~/Scripts/jquery-1.8.2.min.js")
@Scripts.Render("~/Scripts/jquery-ui-1.10.3.js")
@Scripts.Render("~/Scripts/knockout-2.3.0.js")
@Scripts.Render("~/Scripts/jqueryNotification.js")
@Scripts.Render("~/Scripts/jquery.blockUI.js")
@Scripts.Render("~/Scripts/AppCommonScript.js")
    
<form class="form-horizontal">
 
    
    <div class="row">
        <div class="span10">
            <div class="page-header">
                <h2>Create City <small><i class="icon-double-angle-right"></i> </small></h2>
            </div>
        </div>
    </div>

    <div class="row">
        <div class="span5">
            <div class="control-group">
                <label class="control-label" for="FirstName">City Name<span class="required"></span></label>
                <div class="controls">
                    <input id="CityName" type="text" data-bind="value: City_Name" />
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="span10">
            <div class="form-actions">
                <input id="btnCreate" type="button" value="Save" data-bind="click: CreateCity" class="btn btn-primary btn-medium" />
            </div>
        </div>
    </div>
</form>





<script src="@Url.Content("~/Scripts/City/City.js")" type="text/javascript"></script>

