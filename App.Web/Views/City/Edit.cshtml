@Scripts.Render("~/Scripts/jquery.blockUI.js")
@Scripts.Render("~/Scripts/AppCommonScript.js")
    
<h2>Edit City</h2>
<div id='CityEdit'>
    <table class="table table-striped table-hover">
        <tr>
 <th style="display:none">City_Id</th>
            <th>City_Name</th>
        </tr>
        <tbody data-bind="foreach: City">
            <tr>
                <td style="display:none"><input data-bind='value: City_Id' /></td>
                <td>
                    <input data-bind='value: City_Name' />

                </td>
              
                @*<td> <button id="btnCreate" value="Save" data-bind="click: CityListVM.SaveCity" class="btn btn-primary btn-medium"></button></td>*@
                <td>    <input id="btnCreate" type="button" value="Save" data-bind="click: CityListVM.SaveCity" class="btn btn-primary btn-medium" /></td>
            </tr>

        </tbody>
    </table>
</div>
 





<script src="@Url.Content("~/Scripts/City/EditCityById.js")" type="text/javascript"></script>
