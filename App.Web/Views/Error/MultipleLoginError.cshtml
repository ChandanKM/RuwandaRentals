<body>


    <!-- Header -->
    <header class="header navbar navbar-fixed-top" role="banner">
        <!-- Top Navigation Bar -->
        <div class="container">

            <!-- Only visible on smartphones, menu toggle -->
            <ul class="nav navbar-nav">
                <li class="nav-toggle"><a href="javascript:void(0);" title=""><i class="icon-reorder"></i></a></li>
            </ul>

            <!-- Logo -->
            <a class="navbar-brand" href='#'>
                <img src="~/img/Design/logo.png" alt="Rwanda Rentals">
            </a>

        </div>

    </header>
    <!-- /.header -->

    <div id="container">
        <div id="sidebar" class="sidebar-fixed">



        </div>
        <!-- /Sidebar -->1

        <div id="content">
            <div class="container">

                <br />
                <br />
                <!--=== Page Content ===-->

                <section class="content-wrapper main-content clear-fix">
                    <hgroup>
                        <h1 class="text-error">Error.</h1>
                        @if (User.IsInRole("SuperAdmin"))
                        {
                            <h2 class="text-error">You already logged in as Admin,</h2>
                            <h2> Please Logout from admin account to login as this User.</h2>
                            @Html.ActionLink("Click here to logout", "Logoff", "SuperAdmin")
                        }
                        else if (User.IsInRole("Admin"))
                        {
                            <h2 class="text-error">You already logged in as Vendor,</h2>
                            <h2> Please Logout from Vendor account to login as this User.</h2>
                            @Html.ActionLink("Click here to logout", "Logoff", "Vendor")
                        }
                        else if (User.IsInRole("Corporate"))
                        {
                            <h2 class="text-error">You already logged in as Corporate,</h2>
                            <h2> Please Logout from corporate account to login as this User.</h2>
                            @Html.ActionLink("Click here to logout", "Logoff", "Corporate")
                        }
                        else
                        {
                            <h2 class="text-error">You already logged in different User,</h2>
                            <h2> Please Logout, to login as this User.</h2>
                            @Html.ActionLink("Click here to logout", "Logoff", "Signin")
                        }


                    </hgroup>
                </section>
                <!-- /Page Content -->
            </div>
            <!-- /.container -->

        </div>
    </div>



</body>
</html>

