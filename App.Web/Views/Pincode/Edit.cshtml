@model App.Web.ViewModels.PincodeViewModel

@{
    ViewBag.Title = "LMK Maintenance";
}

    
<h2>Edit User</h2>
<div id='UserEdit'>
    <table class="table table-striped table-hover">
        <tr>
 <th style="display:none">PincodeId</th>
            <th>Pincode</th>
  <th>Action</th>
        </tr>
        <tbody data-bind="foreach: Pincodes">
            <tr>
                <td style="display:none"><input data-bind='value: PincodeId' /></td>
                <td>
                    <input data-bind='value: Pincode' />

                </td>
                <td>    <input id="btnCreate" type="button" value="Save" data-bind="click: PincodeListVM.SavePincode" class="btn btn-primary btn-medium" /></td>
            </tr>

        </tbody>
    </table>
</div>
 





<script src="@Url.Content("~/Scripts/Pincode/EditPincodeById.js")" type="text/javascript"></script>
