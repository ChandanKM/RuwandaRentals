<!doctype html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <title>VCNR EazyRentals </title>
    <link rel="shortcut icon" href="~/img/Splitted_Images/LMK%20favicon.ico" type="image/x-icon" />
    <meta name="keywords" content="Last Minute Keys" />
    <meta name="description" content="Last Minute Keys" />

    <link rel="stylesheet" type="text/css" href="~/Content/Consumer/css/bootstrap.css">
    <link rel="stylesheet" type="text/css" href="~/Content/Consumer/css/custom-style.css">
    <link href='http://fonts.googleapis.com/css?family=Roboto:500,100,300,400' rel='stylesheet' type='text/css'>
</head>

<body>
    <!-- Header Section -->
    <div class="header">
        <div class="wrap">
            <div class="logo">
                <a href="@Url.Action("Index","Home")"><img src="~/Content/Consumer/images/logo.png" title="Last Minute Keys" alt="" /></a>
            </div>

            <div class="header-right">
                <ul>
                    @*<li class="help-desc"><a class="help" href="@Url.Action("terms_conditions", "Home")">Help Line</a></li>*@
                    <li class="cont-info"><span class="cnt-desc">Help Line</span> <span class="cnt-number">+91 022 43438150</span> </li>
                    <li>
                        <div class="dropdown user-dropdown">
                            <button class="dropdown-toggle" type="button" id="dropdownMenu1" data-toggle="dropdown" aria-expanded="true">
                                <span class="user-name" id="user_name"><i class="user-icon"></i> <strong data-bind="text:User_Name"></strong></span>
                                <span class="caret"></span>
                            </button>
                            <ul class="dropdown-menu user-profile-dropdown" role="menu" aria-labelledby="dropdownMenu1">
                                <li role="presentation"><a role="menuitem" tabindex="-1" href="@Url.Action("Index", "my_account")">Profile</a></li>
                                @*<li role="presentation"><a role="menuitem" tabindex="-1" href="@Url.Action("Index", "my_account")">Book Room</a></li>*@
                                @* <li role="presentation"><a role="menuitem" tabindex="-1" href="#">Help</a></li>*@
                                <li role="presentation"><a role="menuitem" tabindex="-1" href="@Url.Action("Logoff","Signin")">Logout</a></li>
                            </ul>
                        </div>
                    </li>
                    <li class="app-download"><a href="#"><i class="mobile-icon"></i><span class="app-desc">Book from every where,<br /> Download our Mobile App</span></a></li>
                    <div class="clear"> </div>
                </ul>
            </div>
            <div class="clear"></div>
        </div>
    </div>
    <!-- End Header Section -->
    <!-- Main Section -->
    <div class="main">
        @RenderSection("featured", required: false)
        <section>

            @RenderBody()

        </section>
    </div>
    <!-- End Main Section -->
    <!-- Footer Section -->
    <div class="footer">

        <section>
            <div class="wrap">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-xs-6 col-sm-3 footer-desc">
                            <h3>About VCNR EazyRentals</h3>
                            <ul>
                                <li><a href="@Url.Action("Contact_Us", "Home")">Contact Us</a></li>

                                <li><a href="@Url.Action("AboutUs", "Home")">About Us</a></li>
                            </ul>
                        </div>
                        <div class="col-xs-6 col-sm-3 footer-desc">
                            <h3>Service & Support</h3>
                            <ul>
                                @*<li><a href="#">Manage Bookings</a></li>
                                    <li><a href="@Url.Action("Cancelation", "Home")">Cancellation</a></li>
                                    <li><a href="@Url.Action("Complaints", "Home")">Problems/Complaints</a></li>*@
                                <li><a href="@Url.Action("Faq", "Home")">FAQs</a></li>
                                <li><a href="@Url.Action("Feedback", "Home")">Feedback</a></li>
                            </ul>
                        </div>
                        <div class="clearfix visible-xs-block"></div>
                        <div class="col-xs-6 col-sm-3 footer-desc">
                            <h3>Download Our App</h3>
                            <ul class="app-icons">
                                <li><a href="#"><img src="~/Content/Consumer/images/iphone-app.png" title="Iphone Application" alt="" /></a></li>
                                <li><a href="#"><img src="~/Content/Consumer/images/android-app.png" title="Android Application" alt="" /></a></li>
                            </ul>
                        </div>
                        <div class="col-xs-6 col-sm-3 footer-desc">
                            <h3>Follow Us On</h3>
                            <ul class="socail-icons">
                                <li class="google-plus" title="Google Plus"><a href="#"></a></li>
                                <li class="linkedin" title="Linkedin"><a href="#"></a></li>
                                <li class="pinterest" title="Pinterest"><a href="#"></a></li>
                            </ul>
                            <ul class="socail-icons">
                                <li class="twitter" title="Twitter"><a href="#"></a></li>
                                <li class="facebook" title="Facebook"><a href="#"></a></li>
                                <li class="yahoo" title="Yahoo"><a href="#"></a></li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </section>

        <div class="copy-right">
            <div class="wrap">
                <p class="float-lt"> © Copyright 2015 VCNR EazyRentals. All rights reserved.</p>
                <ul class="float-rt copy-right-nav">
                    <li><a href="#">Help</a></li>
                    <li><a href="#">Privacy</a></li>
                    <li><a href="#">Terms of Use</a></li>
                    <li><a target="_blank" href="http://www.tripadvisor.com/pages/terms.html">© TripAdvisor 2015</a></li>
                    <li><a target="_blank" href="http://www.tripadvisor.com/pages/terms.html">Ratings &amp;  Reviews </a></li>
                    <li><a target="_blank" href="http://www.tripadvisor.com/pages/terms.html"><img src="~/img/tripadvisor_logo.png" /></a></li>


                </ul>

                <div class="clear"></div>
            </div>
        </div>

    </div>
    <!-- End Footer Section -->
    <!-- Scripts -->
    <script src="~/Scripts/jquery-1.11.2.min.js" type="text/javascript"></script>
    <script src="~/Scripts/knockout-3.1.0.debug.js"></script>

    <script src="~/Content/Consumer/js/jquery-ui.js"></script>

    <script src="~/Scripts/jquery.storage.js"></script>
    <script src="~/Scripts/plugins/jquery.cookie.min.js"></script>

    @Scripts.Render("~/Scripts/jqueryNotification.js")
    @Scripts.Render("~/Scripts/jquery.blockUI.js")
    @Scripts.Render("~/Scripts/AppCommonScript.js")

    <!-- Date Picker -->
    <script src="~/Content/Consumer/js/moment.min.js" type="text/javascript"></script>
    <script src="~/Content/Consumer/js/bootstrap.min.js"></script>
    <script src="~/Content/Consumer/js/bootstrap-datetimepicker.min.js"></script>

    <script type="text/javascript" src="~/plugins/datatables/jquery.dataTables.min.js"></script>

    <!-- Styles -->
    <link href="~/Content/Consumer/css/jquery-ui.css" rel="stylesheet" />
    <link href="~/Content/error-notifier.css" rel="stylesheet" type="text/css" />

    @*<link href="~/Content/themes/base/jquery.ui.all.css" rel="stylesheet" type="text/css" />*@


    <script type="text/javascript">
        $(function () {
            $('#checkin').datetimepicker({
                pickTime: false
            });
            $('#checkout').datetimepicker({
                pickTime: false
            });

        });

    </script>

    <!-- Vertical Tab -->
    <script src="~/Content/Consumer/js/easyResponsiveTabs.js"></script>
    <script type="text/javascript">
        $(document).ready(function () {
            $('#VerticalTab').easyResponsiveTabs({
                type: 'vertical', //Types: default, vertical, accordion
                width: 'auto', //auto or any width like 600px
                fit: true // 100% fit in a container
            });
        });
    </script>

    <!-- Slider -->
    <script src="~/Content/Consumer/js/jquery.flexslider.js"></script>

    <script src="~/Scripts/Consumer/my_accountMaster.js"></script>

    @RenderSection("scripts", required: false)
</body>
</html>
