@{
    ViewBag.Title = "RoomRateCal";
    Layout = "~/Views/Shared/_SuperadminLayout.cshtml";
}

<link href="~/Content/error-notifier.css" rel="stylesheet" />
<style>
    .table-bordered {
        table-layout: fixed;
    }

        .table-bordered > thead > tr > th, .table-bordered > thead > tr > td {
            width: 150px;
        }

    #RoomTypeGrid thead tr th {
        background: #036396;
        color: #FFF;
        vertical-align: middle;
        text-align: center;
        border: 1px solid #1E76A5;
    }

        #RoomTypeGrid thead tr th > table {
            background: inherit;
        }

            #RoomTypeGrid thead tr th > table td.price-bg {
                background: #51A351;
            }

            #RoomTypeGrid thead tr th > table td.avail-bg {
                background: #E88400;
            }
</style>
@Scripts.Render("~/Scripts/jqueryNotification.js")
@Scripts.Render("~/Scripts/jquery.blockUI.js")
@Scripts.Render("~/Scripts/AppCommonScript.js")
