    @*<a id="btnhide" onclick="Reload()" style="margin-left: 92.4%;margin-top: -9.2%;" class="btn btn-success">Update</a>*@

    <div class="modal fade" id="modelShowDetail" style="margin-top:10%">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header" style="background:#3E7EB6;">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                    <h4 class="modal-title" style="color:#FFF;">Profit &amp; Loss</h4>
                </div>

                <div class="modal-body">
                    <table class="table table-bordered" style="table-layout:inherit;">
                        <thead><tr><th>Vendor Price</th><th>Camo Price</th><th>Tax</th><th>Net</th><th>CCAvenue Charges</th><th>Margin</th></tr></thead>
                        <tbody data-bind="foreach:Details">
                            <tr>
                                <td data-bind="text:vndramnt"></td>
                                <td data-bind="text:camflg_Amnt"></td>
                                <td data-bind="text:tax_amnt"></td>
                                <td data-bind="text:net_amt"></td>
                                <td data-bind="text:ccavenuecharges"></td>
                                <td data-bind="text:margin"></td>
                            </tr>
                        </tbody>
                    </table>

                </div>
                <div class="modal-footer">

                    <button id="btnModelClose" type="button" class="btn btn-lg btn-primary" data-dismiss="modal">Close</button>

                </div>

            </div><!-- /.modal-content -->
        </div><!-- /.modal-dialog -->
    </div>
