<!--=== Page Header ===-->

<div class="widget box">
    <div class="widget-header">
        <h4><i class="icon-reorder"></i> Admin Info</h4>
    </div>
    <div class="widget-content">
        <form class="form-horizontal row-border" action="#">

            <div class="row" id="divVendorCreateProfile">
                <div class="span5" data-bind="foreach:ProfileData">

               
                    <div class="form-group">
                        <label class="col-md-3 control-label">Company Name<b style="color:red">*</b></label>
                        <div class="col-md-9">

                            @*<input type="text" name="regular" class="form-control" data-bind=" text: vndr_id">*@
                            <input id="Company_Title" class="form-control input-width-xlarge" type="text" data-bind="value:Company_Title" />
                        </div>
                    </div>


                    <div class="form-group">
                        <label class="col-md-3 control-label">Owned by<b style="color:red">*</b></label>
                        <div class="col-md-9">

                            @*<input type="text" name="regular" class="form-control" data-bind=" text: vndr_id">*@
                            <input id="Vndr_Cinno" class="form-control input-width-xlarge" type="text" data-bind="value:Owned_By" />
                        </div>
                    </div>



                    <div class="form-group">
                        <label class="col-md-3 control-label">CIN Number<b style="color:red">*</b></label>
                        <div class="col-md-9">

                            @*<input type="text" name="regular" class="form-control" data-bind=" text: vndr_id">*@
                            <input id="Vndr_Addr1" class="form-control input-width-xlarge" type="text" data-bind="value:CIN_Number" />
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-3 control-label">Address Line1<b style="color:red">*</b></label>
                        <div class="col-md-9">

                            @*<input type="text" name="regular" class="form-control" data-bind=" text: vndr_id">*@
                            <input id="Cityvalue" class="form-control input-width-xlarge" type="text" data-bind="value:Adr1" />
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-3 control-label">Address Line2<b style="color:red">*</b></label>
                        <div class="col-md-9">

                            @*<input type="text" name="regular" class="form-control" data-bind=" text: vndr_id">*@
                            <input id="Cityvalue" class="form-control input-width-xlarge" type="text" data-bind="value:Adr2" />
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-3 control-label">Location<b style="color:red">*</b></label>
                        <div class="col-md-9">

                            @*<input type="text" name="regular" class="form-control" data-bind=" text: vndr_id">*@
                            <input id="pincodevalue" class="form-control input-width-xlarge" type="text" data-bind="value:Location" />
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-3 control-label">City<b style="color:red">*</b></label>
                        <div class="col-md-9">

                            @*<input type="text" name="regular" class="form-control" data-bind=" text: vndr_id">*@
                            <input id="pincodevalue" class="form-control input-width-xlarge" type="text" data-bind="value:City" />
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-3 control-label">TIN Id<b style="color:red">*</b></label>
                        <div class="col-md-9">

                            @*<input type="text" name="regular" class="form-control" data-bind=" text: vndr_id">*@
                            <input id="pincodevalue" class="form-control input-width-xlarge" type="text" data-bind="value:Tin_id" />
                        </div>
                    </div>
                    <div class="widget-header">
                        <h4><i class="icon-reorder"></i>Contact Info</h4>
                    </div>
                    <div class="form-group">
                        <label class="col-md-3 control-label">Phone<b style="color:red">*</b></label>
                        <div class="col-md-9">

                            @*<input type="text" name="regular" class="form-control" data-bind=" text: vndr_id">*@
                            <input id="Phone" onkeypress="if ( isNaN( String.fromCharCode(event.keyCode) )) return false;" class="form-control input-width-xlarge" type="text" data-bind="value:Phone" />
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-3 control-label">Mobile<b style="color:red">*</b></label>
                        <div class="col-md-9">

                            @*<input type="text" name="regular" class="form-control" data-bind=" text: vndr_id">*@
                            <input id="Mobile" onkeypress="if ( isNaN( String.fromCharCode(event.keyCode) )) return false;" class="form-control input-width-xlarge" type="text" data-bind="value:Mobile" />
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-3 control-label">Email<b style="color:red">*</b></label>
                        <div class="col-md-9">

                            @*<input type="text" name="regular" class="form-control" data-bind=" text: vndr_id">*@
                            <input id="Email" class="form-control input-width-xlarge" type="text" data-bind="value:Email" />

                        </div>
                    </div>
                    <div class="widget-header">
                        <h4><i class="icon-reorder"></i>SMS Setup</h4>
                    </div>
                    <div class="form-group">
                        <label class="col-md-3 control-label">SMS URL<b style="color:red">*</b></label>
                        <div class="col-md-9">

                            @*<input type="text" name="regular" class="form-control" data-bind=" text: vndr_id">*@
                            <input id="Sms_Url" class="form-control input-width-xlarge" type="text" data-bind="value:Sms_Url" />
                        </div>
                    </div>
                    <div class="widget-header">
                        <h4><i class="icon-reorder"></i>Email Setup</h4>
                    </div>
                    <div class="form-group">
                        <label class="col-md-3 control-label">Email ID<b style="color:red">*</b></label>
                        <div class="col-md-9">

                            @*<input type="text" name="regular" class="form-control" data-bind=" text: vndr_id">*@
                            <input id="Sms_Url" class="form-control input-width-xlarge" type="text" data-bind="value:SetupEmail" />
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-3 control-label">Password<b style="color:red">*</b></label>
                        <div class="col-md-9">

                            @*<input type="text" name="regular" class="form-control" data-bind=" text: vndr_id">*@
                            <input id="Sms_Url" class="form-control input-width-xlarge" type="text" data-bind="value:Password" />
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-3 control-label">SMTP Port<b style="color:red">*</b></label>
                        <div class="col-md-9">

                            @*<input type="text" name="regular" class="form-control" data-bind=" text: vndr_id">*@
                            <input id="Sms_Url" class="form-control input-width-xlarge" type="text" data-bind="value:SMTP" />
                        </div>
                    </div>
                    <div class="modal-footer" style="margin-left: 0.5%; margin-right: 0.5%;">
                        <input id="btnSave" type="button" value="Update" data-bind="click: Update" class="btn btn-lg btn-primary" />
                        <input id="btnReset" type="button" value="Cancel"  onclick="window.history.back()" class="btn btn-lg btn-danger" />
                        @*@Html.ActionLink("Back to List", "Bind", "User")*@

                    </div>


                </div>
            </div>
    </div>

    

</div>



<script src="@Url.Content("~/Scripts/SystemProfile/EditSysProfile.js")" type="text/javascript"></script>

@*<script src="~/Scripts/SystemProfile/EditSysProfile.js"></script>*@
<script src="~/Scripts/SuperAdminMaster.js"></script>