<meta charset="utf-8">
<title>Rwanda :: Manage Property</title>
<link rel="shortcut icon" href="~/img/Splitted_Images/favicon.ico" type="image/x-icon" />
<meta name="keywords" content="Last Minute Keys" />
<meta name="description" content="Last Minute Keys" />
@*Scripts for create*@

<!-- DataTables -->

<form class="form-horizontal">

    <div class="crumbs">
        <ul id="breadcrumbs" class="breadcrumb">
            <li>
                <i class="icon-home"></i>
                <a href="#">Home</a>
            </li>

            <li class="current">
                Bookings
            </li>


        </ul>

    </div>
    <div class="row">
        <div class="col-md-12">
            <div class="widget box">
                <div class="widget-header">
                    <h4><i class="icon-reorder"></i>Bookings</h4>
                    <div class="toolbar no-padding">
                        <div class="btn-group">
                            <span class="btn btn-xs widget-collapse"><i class="icon-angle-down"></i></span>
                        </div>
                    </div>
                </div>

                <div class="widget-content no-padding">
                    <table class="table table-striped table-bordered table-hover table-checkable BookingsDT">
                        <thead>
                            <tr>

                                <th>
                                    Booking ID
                                </th>
                                <th>
                                    Property
                                </th>
                                <th>
                                    Room Type
                                </th>
                                <th>
                                    Check In
                                </th>
                                <th>
                                    Name
                                </th>
                                <th>
                                    Mobile
                                </th>
                                @*<th>
                                    Options
                                </th>*@

                            </tr>
                        </thead>
                        <tbody data-bind="foreach: Bookings" id="BookingsDT">
                            <tr>

                                <td><a href="#"><span data-bind="text: Invce_Num,click:DT"></span></a></td>
                                @*<td data-bind="text: Invce_Num"></td>*@
                                <td data-bind="text: Prop_Name"></td>
                                <td data-bind="text: Room_Type"></td>
                                <td data-bind="text: Checkin"></td>
                                <td data-bind="text: Name"></td>
                                <td data-bind="text: Mobile"></td>

                                @*<td>
                                    <a style="cursor:pointer;" data-bind="click:DT"> Details</a>
                                    <a style="cursor:pointer" data-bind="click: deleteProperty"><img src="../../img/Splitted_Images/disable_button.png" /></a>

                                                                </td>*@
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</form>
<script src="~/Scripts/jquery.storage.js"></script>
<script src="~/Scripts/Master.js"></script>
<script src="~/Scripts/Booking/UpcomingBookings.js"></script>


