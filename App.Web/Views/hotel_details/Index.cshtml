                                <ul class="booking_details">
                                    <li>
                                        <div class="checkin-details">
                                            <h4 class="checkin-h4">Check In</h4>
                                            <div class="week-details">
                                                <span class="checkin-date" data-bind="text:Room_Checkin"> </span>
                                            </div>
                                        </div>
                                    </li>
                                    <li>
                                        <div class="checkin-details">
                                            <h4>Check Out</h4>
                                            <div class="week-details week-details2">
                                                <span class="checkin-date" data-bind="text:Room_Checkout"> </span>
                                            </div>
                                        </div>
                                    </li>
                                    <div class="clear"></div>
                                </ul>

                            </div>

                            <div class="room-details-btn" style="display:none"><a title="View Room Details" href="#" data-bind="click:ViewRoomDetails"><i class="book-now-icon"></i> View Room Details</a></div>
                            <div class="clear"></div>
                        </div>
                        <div class="clear"></div>
                    </section>

